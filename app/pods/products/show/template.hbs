<body class="news">
  <header>
    <div class="nav">
        <div class="dropdown-content">
          <header>
            <div class="mainnav">
              <ul class="active">
                <img src='/assets/avatar.png'>
                <li onclick='console.log("Button Clicked")' class="publisher"><a href="#">{{link-to 'Data Publisher' 'products'}}</a></li>
              </ul>
            </div>
          </header>
        </div>
    </div>
